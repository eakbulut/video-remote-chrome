{
  "manifest_version": 2,
  "name": "Video Remote",
  "version": "0.0.1",
  "description": "Play/Pause video from VSCode",
  "permissions": [
    "alarms",
    "activeTab",
    "webRequest",
    "ws://*/*",
    "wss://*/*"
  ],
  "content_scripts": [
      {
          "matches": [
              "*://www.youtube.com/*",
              "*://learning.anaconda.cloud/*"
          ],
          "js": ["content.js"]
      }
  ],
  "web_accessible_resources": ["content.js"]
}